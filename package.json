{
  "name": "foxmulder",
  "version": "2.5.0",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "mocha --opts mocha.opts",
    "test-ci": "mocha --opts mocha.opts --reporter mocha-junit-reporter **/*.spec.js",
    "coverage": "nyc --reporter=text-summary --reporter=html npm run test",
    "lint": "prettier --single-quote --trailing-comma none --use-tabs --tab-width 4 \"{webpack.config,{server,web,tests}/**/*}.+(js|json|scss)\" --write",
    "build": "cross-env NODE_ENV=production webpack --display-exclude --display-optimization-bailout --colors",
    "start": "cross-env NODE_ENV=development webpack-dev-server --colors"
  },
  "license": "MIT",
  "engines": {
    "node": ">6.0.0"
  },
  "dependencies": {
