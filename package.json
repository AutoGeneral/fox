    "angular": "1.6.6",
    "angular-socket-io": "0.7.0",
    "aws-sdk": "2.100.0",
    "dygraphs": "1.1.1",
    "eureka-js-client": "4.3.0",
    "gauss": "0.2.12",
    "lokijs": "1.5.0",
    "mkdirp": "0.5.1",
    "moment": "2.18.1",
    "mysql": "2.14.1",
    "ng-dialog": "1.4.0",
    "nodemailer": "2.6.4",
    "nodemailer-ses-transport": "1.5.1",
    "proxy-agent": "2.1.0",
    "request-promise": "3.0.0",
